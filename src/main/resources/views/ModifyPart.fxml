<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ModifyPartController">
    <children>
        <Label layoutX="24.0" layoutY="26.0" text="Modify Part" />
        <RadioButton fx:id="ModifyPartInHouse" layoutX="128.0" layoutY="25.0" mnemonicParsing="false" onAction="#OnInHouseButtonClick" selected="true" text="In-House">
            <toggleGroup>
                <ToggleGroup fx:id="PartSource" />
            </toggleGroup></RadioButton>
        <RadioButton fx:id="ModifyPartOutsourced" layoutX="261.0" layoutY="25.0" mnemonicParsing="false" onAction="#OnOutsourcedButtonClick" text="Outsourced" toggleGroup="$PartSource" />
        <Label layoutX="36.0" layoutY="118.0" text="ID" />
        <Label layoutX="23.0" layoutY="162.0" text="Name" />
        <Label layoutX="25.0" layoutY="200.0" text="Inv" />
        <Label layoutX="19.0" layoutY="252.0" text="Price/Cost" />
        <Label layoutX="22.0" layoutY="290.0" text="Max" />
        <Label fx:id="ModifyPartMachineIdLabel" layoutX="21.0" layoutY="327.0" text="Machine ID" />
        <TextField fx:id="ModifyPartId" editable="false" layoutX="140.0" layoutY="114.0" />
        <TextField fx:id="ModifyPartName" layoutX="140.0" layoutY="158.0" />
        <TextField fx:id="ModifyPartInv" layoutX="140.0" layoutY="204.0" />
        <TextField fx:id="ModifyPartPriceCost" layoutX="140.0" layoutY="248.0" />
        <TextField fx:id="ModifyPartMax" layoutX="140.0" layoutY="286.0" />
        <TextField fx:id="ModifyPartMachineId" layoutX="140.0" layoutY="323.0" />
        <Label layoutX="325.0" layoutY="290.0" text="Min" />
        <TextField fx:id="ModifyPartMin" layoutX="383.0" layoutY="286.0" />
        <Button fx:id="ModifyPartSaveButton" layoutX="407.0" layoutY="362.0" mnemonicParsing="false" onAction="#OnSaveButtonClick" text="Save" />
        <Button fx:id="ModifyPartCancelButton" layoutX="492.0" layoutY="362.0" mnemonicParsing="false" onAction="#OnCancelButtonClick" text="Cancel" />
    </children>
</AnchorPane>
